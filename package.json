{
  "name": "maihouses",
  "private": true,
  "version": "1.0.7",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --port 4173",
    "deploy": "npm run build && gh-pages -d docs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "speedup:start": "bash scripts/speedup-control.sh start",
    "speedup:stop": "bash scripts/speedup-control.sh stop",
    "speedup:status": "bash scripts/speedup-control.sh status",
    "speedup:log": "bash scripts/speedup-control.sh log",
    "lint:ui": "bash ./check-ui.sh",
    "dev:force": "vite --force"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.83.0",
    "cloudinary": "^1.41.0",
    "lucide-react": "^0.554.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.26.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "gh-pages": "^6.1.1",
    "html-minifier-terser": "^7.2.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.47",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.21",
    "vite-plugin-checker": "^0.11.0",
    "vitest": "^2.0.0"
  }
}
