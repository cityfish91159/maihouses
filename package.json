{
  "name": "maihouses",
  "private": true,
  "version": "1.0.7",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite",
    "dev:story": "vite --clearScreen false --open /maimai-story.html",
    "build": "tsc && vite build",
    "preview": "vite preview --port 4173",
    "dev:maimai-story": "vite --open /maihouses/maimai-story.html",
    "deploy": "npm run build && gh-pages -d dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate:schema": "tsx scripts/generate-json-schema.ts",
    "verify:seed": "tsx scripts/verify-seed-strict.ts",
    "validate:property": "tsx scripts/validate-property-types.ts",
    "check:ssot": "tsx scripts/verify-seed-strict.ts",
    "test:phase4": "vitest run public/js/__tests__/property-phase4.test.js",
    "test:telemetry": "vitest run public/js/__tests__/telemetry.test.js",
    "phase4:flicker": "tsx scripts/phase4/flicker-visual.ts",
    "test:phase5": "tsx scripts/phase5/e2e-phase5.ts",
    "speedup:start": "bash scripts/speedup-control.sh start",
    "speedup:stop": "bash scripts/speedup-control.sh stop",
    "speedup:status": "bash scripts/speedup-control.sh status",
    "speedup:log": "bash scripts/speedup-control.sh log",
    "lint:ui": "bash ./check-ui.sh",
    "gate": "node scripts/quality-gate.cjs",
    "police": "node scripts/police.cjs",
    "dev:force": "vite --force",
    "prepare": "husky"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.83.0",
    "@tanstack/react-query": "^5.90.10",
    "@types/cookie": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "browser-image-compression": "^2.0.2",
    "canvas-confetti": "^1.9.4",
    "cloudinary": "^1.41.0",
    "clsx": "^2.1.1",
    "cookie": "^1.1.0",
    "exif-js": "^2.3.0",
    "heic2any": "^0.0.4",
    "immer": "^10.2.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.554.0",
    "openai": "^6.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.26.2",
    "react-share": "5.2.2",
    "react-window": "^2.2.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "uuid": "^9.0.1",
    "zod": "^4.2.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/exif-js": "^2.3.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/parser": "^8.13.0",
    "@vercel/node": "^5.5.13",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eruda": "^3.4.3",
    "eslint": "^9.12.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "gh-pages": "^6.1.1",
    "html-minifier-terser": "^7.2.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "playwright-chromium": "^1.57.0",
    "postcss": "^8.4.47",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.21",
    "vite-plugin-checker": "^0.11.0",
    "vitest": "^2.0.0",
    "zod-to-json-schema": "^3.25.0"
  }
