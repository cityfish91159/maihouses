{
  "redirects": [
    { "source": "/", "destination": "/maihouses", "permanent": false }
  ],
  "rewrites": [
    { "source": "/maihouses", "destination": "/" },
    { "source": "/maihouses/(.*)", "destination": "/$1" }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20.x",
      "memory": 128,
      "maxDuration": 10
    }
  }
}
